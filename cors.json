    "allowedHeaders": [
        "Content-Type",
        "Authorization",
        "X-Requested-With",
        "Accept",
        "Origin",
        "Access-Control-Request-Method",
        "Access-Control-Request-Headers"
    ],
    "exposeHeaders": [
        "Content-Length",
        "Content-Type"
    ]
}