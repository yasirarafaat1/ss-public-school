{
    "responseHeader": [
        "Content-Type",
        "Authorization",
        "Access-Control-Allow-Origin",
        "Cache-Control",
        "Content-Length"
    ],
    "allowedOrigins": [
        "http://localhost:5173",
        "https://your-production-domain.com",
        "https://picsum.photos"
    ],
    "allowedMethods": [
        "GET",
        "POST",
        "PUT",
